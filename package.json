{
  "name": "probablerobot-net",
  "private": true,
  "scripts": {
    "prestart": "rm -rf public",
    "start": "npm-run-all --parallel start:*",
    "start:eleventy": "eleventy --serve",
    "start:postcss": "postcss src/site/_includes/css --dir public/css --watch",
    "prebuild": "rm -rf public",
    "build": "npm-run-all --parallel build:*",
    "build:eleventy": "eleventy --config=.eleventy.cjs",
    "build:postcss": "postcss src/site/_includes/css --dir public/css",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "9.1.1",
    "@prettier/plugin-xml": "3.4.1",
    "eslint": "9.1.1",
    "globals": "15.0.0",
    "prettier": "3.2.5"
  },
  "dependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "cssnano": "6.1.2",
    "luxon": "3.4.4",
    "markdown-it": "14.1.0",
    "markdown-it-footnote": "4.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0"
  },
  "packageManager": "pnpm@8.15.8+sha256.691fe176eea9a8a80df20e4976f3dfb44a04841ceb885638fe2a26174f81e65e"
}
